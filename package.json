{
  "name": "gitpair",
  "version": "1.3.1",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "author": "Benoit d'Oncieu <bdoncieu@gmail.com>",
  "description": "Allow you to commit with more than one person, e.g. Pair programming or Mob programming.",
  "homepage": "https://github.com/bdo/gitpair.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdo/gitpair.git"
  },
  "keywords": [
    "git",
    "pair-programming",
    "mob-programming"
  ],
  "bugs": {
    "url": "https://github.com/bdo/gitpair.git/issues"
  },
  "main": "./src/index.js",
  "bin": {
    "gitpair": "./bin/gitpair.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "webpack -p",
    "watch": "npm run build -- --watch",
    "lint": "eslint .",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/testSetup"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-source-map-support": "^2.0.1",
    "chalk": "^2.4.2",
    "clipboardy": "^2.0.0",
    "core-js": "^3.0.1",
    "debug": ">=2.6.9",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
