{
  "name": "gitpair",
  "version": "1.5.4",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "author": "Benoit d'Oncieu <bdoncieu@gmail.com>",
  "description": "Allow you to commit with more than one person, e.g. Pair programming or Mob programming.",
  "homepage": "https://github.com/bdo/gitpair.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/bdo/gitpair.git"
  },
  "keywords": [
    "git",
    "pair-programming",
    "mob-programming"
  ],
  "bugs": {
    "url": "https://github.com/bdo/gitpair.git/issues"
  },
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "gitpair": "./bin/gitpair.js",
    "git-pair": "./bin/gitpair.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "transform": {},
    "setupFiles": [
      "./src/testSetup"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "clipboardy": "^3.0.0",
    "debug": "^4.3.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "semantic-release": "^22.0.12"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  }
}
